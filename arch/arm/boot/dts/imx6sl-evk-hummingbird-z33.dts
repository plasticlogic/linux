		vpos_pwrup = <2>;
		gvdd_pwrup = <5>;
		gvdd_pwrdn = <0>;
		vpos_pwrdn = <0>;
		gvee_pwrdn = <0>;
		vneg_pwrdn = <1>;
		gpio_pmic_pwrgood = <&display_bank2_gpio 6 0>;
		gpio_pmic_vcom_ctrl = <&display_bank2_gpio 2 0>;
		gpio_pmic_wakeup = <&display_bank2_gpio 0 0>;
		gpio_pmic_v3p3 = <&display_bank2_gpio 3 0>;
		gpio_pmic_intr = <&display_bank2_gpio 7 0>;
