&ecspi1 {
	fsl,spi-num-chipselects = <2>;
	/* lcd_dat4 lcd_dat5 */
	cs-gpios =  <&gpio2 24 0>, 
				<&gpio2 25 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1>;
	status = "okay";
	/* NVM Flash on Plasticlogic Display */
	nvm1@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		/*compatible = "st,m25p32";*/
		compatible = "mx25l4005a";
		reg = <0>;
		spi-max-frequency = <2000000>;
		mode = <0>;
		waveform@35000 {
			reg = <0x00035000 0x0003AFFF>;
			label = "waveform0";
			read-only;
		};
		partno@70000 {
			reg = <0x00070000 0x0000000F>;
			label = "partnumber0";
			read-only;
		};
		vcom@70010 {
			reg = <0x00070010 0x0000000F>;
			label = "vcom0";
			read-only;
		};
		wfname@70020 {
			reg = <0x00070020 0x0000001F>;
			label = "wfname0";
			read-only;
		};
		wflength@700AC {
			reg = <0x000700AC 0x00000004>;
			label = "wflength0";
			read-only;
		};
	};
	nvm1@1 {
		#address-cells = <1>;
		#size-cells = <1>;
		/*compatible = "st,m25p32";*/
		compatible = "mx25l4005a";
		reg = <1>;
		spi-max-frequency = <2000000>;
		mode = <0>;
		waveform@35000 {
			reg = <0x00035000 0x0003AFFF>;
			label = "waveform1";
			read-only;
		};
		partno@70000 {
			reg = <0x00070000 0x0000000F>;
			label = "partnumber1";
			read-only;
		};
		vcom@70010 {
			reg = <0x00070010 0x0000000F>;
			label = "vcom1";
			read-only;
		};
		wfname@70020 {
			reg = <0x00070020 0x0000001F>;
			label = "wfname1";
			read-only;
		};
		wflength@700AC {
			reg = <0x000700AC 0x00000004>;
			label = "wflength1";
			read-only;
		};
		
	};					
	
};


&epdc {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_epdc_0>;
        V3P3_1-supply = <&V3P31_reg>;
        VCOM1-supply = <&VCOM1_reg>;
        DISPLAY1-supply = <&DISPLAY1_reg>;
		V3P3_2-supply = <&V3P32_reg>;
        VCOM2-supply = <&VCOM2_reg>;
        DISPLAY2-supply = <&DISPLAY2_reg>;

	VCOM_SW1_gpio = <&display_bank1_gpio 1 0>;
	VCOM_SW2_gpio = <&display_bank2_gpio 1 0>;
	VS_LOW_SW1_gpio = <&display_bank1_gpio 4 0>;
	VS_HIGH_SW1_gpio = <&display_bank1_gpio 5 0>;
	VS_LOW_SW2_gpio = <&display_bank2_gpio 4 0>;
	VS_HIGH_SW2_gpio = <&display_bank2_gpio 5 0>;
        status = "okay";
};

&fec {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&pinctrl_fec>;
	pinctrl-1 = <&pinctrl_fec_sleep>;
	phy-mode = "rmii";
	status = "okay";
};

&gpc {
	/* use ldo-bypass, u-boot will check it and configure */
	fsl,ldo-bypass = <1>;
	/* watchdog select of reset source */
	fsl,wdog-reset = <1>;
};

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

	pmic: pfuze100@08 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <300000>;
				regulator-max-microvolt = <1875000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3a_reg: sw3a {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw3b_reg: sw3b {
				regulator-min-microvolt = <400000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <3300000>;
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5000000>;
				regulator-max-microvolt = <5150000>;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <3000000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-boot-on;
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
				regulator-always-on;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <800000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};
		};
	};

	elan@10 {
		compatible = "elan,elan-touch";
		reg = <0x10>;
		interrupt-parent = <&gpio2>;
		interrupts = <10 2>;
		gpio_elan_cs = <&gpio2 9 0>;
		gpio_elan_rst = <&gpio4 4 0>;
		gpio_intr = <&gpio2 10 0>;
		status = "disabled";
	};

	mma8450@1c {
		compatible = "fsl,mma8450";
		reg = <0x1c>;
		status = "disabled";
	};

	/* PL ADC device */
	ads1015@49 {
		compatible = "ti,ads1015";
		reg = <0x49>;
		#address-cells = <1>;
		#size-cells = <0>;

		/*VCOM1*/
		channel@4 {
			reg = <4>;
			ti,gain = <2>;
			ti,datarate = <6>;
		};
		/*VCOM2*/
		channel@5 {
			reg = <5>;
			ti,gain = <2>;
			ti,datarate = <2>;
		};
		/*DISP1 current*/
		channel@6 {
			reg = <6>;
			ti,gain = <2>;
			ti,datarate = <0>;
		};
		/*DISP2 current*/
		channel@7 {
			reg = <7>;
			ti,gain = <2>;
			ti,datarate = <4>;
		};
	};
	
	/* PL GPIO Port Expander for Display 1*/
		/* HV_EN1 */
		/* COM_SW1 */
		/* COM_EN1 */
		/* Disp1_PWR */
		/* VSL_SW1 */
		/* VSH_SW1 */
		/* HV_POK1 */
		/* PMIC1_FLT */
	display_bank1_gpio: gpio@20 {
		compatible = "nxp,pcf8574";
		reg = <0x20>;
		/*interrupt-parent = <&i2c1>;*/
		/*interrupts = <3 0>;*/
		gpio-controller;
		#gpio-cells = <2>;
		/*gpio-ranges = <&pinctrl_display_bank1 0 0 8>;*/
		/*interrupt-controller;*/
		/*#interrupt-cells = <2>;*/
	};

	max17135_1@48 {
		compatible = "maxim,max17135" , "maxim,max17135-sensor";
		reg = <0x48>;
		vneg_pwrup = <0>;
		gvee_pwrup = <0>;
		vpos_pwrup = <2>;
		gvdd_pwrup = <5>;
		gvdd_pwrdn = <0>;
		vpos_pwrdn = <0>;
		gvee_pwrdn = <0>;
		vneg_pwrdn = <1>;
		gpio_pmic_pwrgood = <&display_bank1_gpio 6 0>;
		gpio_pmic_vcom_ctrl = <&display_bank1_gpio 2 0>;
		gpio_pmic_wakeup = <&display_bank1_gpio 0 0>;
		gpio_pmic_v3p3 = <&display_bank1_gpio 3 0>;
		gpio_pmic_intr = <&display_bank1_gpio 7 0>;
		status = "okay";
		sensors {
			INTERNAL1_sensor: INTERNAL {
				sensor-name = "INTERNAL_1";
			};
			EXTERNAL1_sensor: EXTERNAL {
				sensor-name = "EXTERNAL_1";
			};
		};
		regulators {
			DISPLAY1_reg: DISPLAY {
				regulator-name = "DISPLAY_1";
			};

			GVDD1_reg: GVDD {
				regulator-name = "GVDD_1";
			};

			GVEE1_reg: GVEE {
				regulator-name = "GVEE_1";
			};

			HVINN1_reg: HVINN {
				regulator-name = "HVINN_1";
			};

			HVINP1_reg: HVINP {
				regulator-name = "HVINP_1";
			};

			VCOM1_reg: VCOM {
				regulator-name = "VCOM_1";
				regulator-min-microvolt = <0xffbe0178>;
				regulator-max-microvolt = <0xfff85ee0>;
			};

			VNEG1_reg: VNEG {
				regulator-name = "VNEG_1";
			};

			VPOS1_reg: VPOS {
				regulator-name = "VPOS_1";
			};

			V3P31_reg: V3P3 {
				regulator-name = "V3P3_1";
			};
		};
	};

};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	codec: wm8962@1a {
		compatible = "wlf,wm8962";
		reg = <0x1a>;
		clocks = <&clks IMX6SL_CLK_EXTERN_AUDIO>;
		DCVDD-supply = <&vgen3_reg>;
		DBVDD-supply = <&reg_aud3v>;
		AVDD-supply = <&vgen3_reg>;
		CPVDD-supply = <&vgen3_reg>;
		MICVDD-supply = <&reg_aud3v>;
		PLLVDD-supply = <&vgen3_reg>;
		SPKVDD1-supply = <&reg_aud4v>;
		SPKVDD2-supply = <&reg_aud4v>;
		amic-mono;
	};
